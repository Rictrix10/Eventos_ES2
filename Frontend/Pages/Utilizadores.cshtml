@page
@model Frontend.Pages.UtilizadoresModel
@{
    ViewData["Title"] = "Lista de utilizadores";
    
}
<h1>Utilizadores</h1>
<p>Welcome to the page where all users <are></are> listed</p>
<table style="width: 100%">
    <tr>
        <th>Username</th>
        <th>Password</th>
        <th>Nome</th>
        <th>Email</th>
        <th>Telefone</th>
    </tr>
    @foreach (var user in Model.Utilizador)
    {
        <tr>
            <td>@user.Username</td>
            <td>@user.Password</td>
            <td>@user.Nome</td>
            <td>@user.Email</td>
            <td>@user.Telefone</td>
        </tr>
    }
</table>